{
  "$schema": "https://oxc-project.github.io/oxlint/config-schema.json",
  "extends": ["oxlint:recommended"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-base-to-string": "off"
  },
  "ignorePatterns": [
    "types/**",
    "sandbox/**"
  ]
}
