{
  "name": "@dxv/cli",
  "version": "3.0.0",
  "description": "A convenient command-line tool that wraps eslint, dprint, tsc, depcheck, and npm-check-updates",
  "main": "index.js",
  "type": "module",
  "bin": {
    "dxv": "bin/index.js"
  },
  "scripts": {
    "lint": "./bin/index.js lint -c .config/eslint.config.js",
    "fmt": "./bin/index.js fmt -c .config/dprint.jsonc",
    "type": "./bin/index.js type -c .config/tsconfig.json",
    "deps": "./bin/index.js deps -c .config/.depcheckrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sethvincent/dxv.git"
  },
  "author": "sethvincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sethvincent/dxv/issues"
  },
  "homepage": "https://github.com/sethvincent/dxv#readme",
  "dependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.0",
    "@types/eslint": "^9.6.1",
    "@types/find-node-modules": "^2.1.2",
    "@types/node": "^24.0.8",
    "cosmiconfig": "^9.0.0",
    "dedent": "^1.6.0",
    "depcheck": "^1.4.7",
    "dprint": "^0.50.1",
    "eslint": "^9.30.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "execa": "^9.6.0",
    "find-node-modules": "^2.1.3",
    "globals": "^16.2.0",
    "mri": "^1.2.0",
    "npm-check-updates": "^18.0.1",
    "ora": "^8.2.0",
    "outdent": "^0.8.0",
    "read-package-json-fast": "^4.0.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3"
  }
}
